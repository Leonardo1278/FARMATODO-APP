AFTER\040INSERT\040ON\040incidencias_facincidences
FOR\040EACH\040ROW
BEGIN
\040\040\040\040INSERT\040INTO\040general_data\040(
\040\040\040\040\040\040\040\040incidence_id,\040location_name,
\040\040\040\040\040\040\040\040main_section,\040inner_section,
\040\040\040\040\040\040\040\040photo_1,\040photo_2,
\040\040\040\040\040\040\040\040photo_4,\040
\040\040\040\040\040\040\040\040description,
\040\040\040\040\040\040\040\040urgency,\040registro_fecha_hora
\040\040\040\040)
\040\040\040\040SELECT
\040\040\040\040\040\040\040\040f.id_incidence,\040d.location_name,
\040\040\040\040\040\040\040\040d.main_section,\040d.inner_section,
\040\040\040\040\040\040\040\040CASE
\040\040\040\040\040\040\040\040\040\040\040\040WHEN\040f.photo_1\040IS\040NOT\040NULL\040THEN\040\040f.photo_1\040\040
\040\040\040\040\040\040\040\040\040\040\040\040ELSE\040'✘'
\040\040\040\040\040\040\040\040END\040AS\040photo_1,
\040\040\040\040\040\040\040\040CASE
